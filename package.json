{
  "name": "tracenet-blockchain",
  "version": "1.1.1",
  "description": "Blockchain ecosystem with Delegated Proof of Activity consensus",
  "bin": {
    "tracenet": "./dist/cli/index.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:all": "tsc && cd frontend && npm run build",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "start:node": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:load": "jest --testPathPattern=tests/load",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "blockchain",
    "dpoa",
    "consensus",
    "cryptocurrency",
    "wallet"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/storage": "^7.18.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.40.3",
    "@opentelemetry/exporter-trace-otlp-http": "^0.45.1",
    "@opentelemetry/sdk-node": "^0.45.1",
    "@scure/bip32": "^1.3.3",
    "@scure/bip39": "^1.2.2",
    "@supabase/supabase-js": "^2.84.0",
    "bcrypt": "^5.1.1",
    "bip39": "^3.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "geoip-lite": "^1.4.10",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "level": "^8.0.0",
    "pg": "^8.11.3",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.8.3",
    "tweetnacl": "^1.0.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}